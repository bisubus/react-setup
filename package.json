{
  "name": "react-setup",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "description": "",
  "keywords": [],
  "author": "bisubus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bisubus/react-setup.git"
  },
  "type": "module",
  "scripts": {
    "build": "npm run --workspaces build",
    "fix": "run-p -c fix:**",
    "fix:root": "eslint --fix .",
    "fix:workspaces": "npm run --workspaces fix",
    "lint": "run-p -c lint:**",
    "lint:root": "eslint .",
    "lint:workspaces": "npm run --workspaces lint",
    "test": "npm run --workspaces test",
    "versions": "run-s \"versions:* -- {1}\" --",
    "versions:core": "npm run version --workspace=@react-setup/core",
    "versions:hooks": "npm run version --workspace=@react-setup/hooks",
    "versions:root": "git add packages/*/package.json package-lock.json && git commit -m \"Update versions\" && npm version"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@testing-library/jest-dom": "~6.6.4",
    "@testing-library/react": "~16.3.0",
    "@types/node": "~24.2.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.30.1",
    "eslint-config-prettier": "~10.1.8",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-prettier": "~5.5.4",
    "eslint-plugin-react-dom": "~1.52.3",
    "eslint-plugin-react-hooks": "~5.2.0",
    "eslint-plugin-react-refresh": "~0.4.20",
    "eslint-plugin-react-x": "~1.52.3",
    "eslint-plugin-simple-import-sort": "~12.1.1",
    "globals": "^16.3.0",
    "jsdom": "~26.1.0",
    "npm-run-all2": "~8.0.4",
    "prettier": "~3.6.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "terser": "~5.43.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vite-plugin-dts": "~4.5.4",
    "vite-plugin-no-bundle": "~4.0.0",
    "vitest": "~3.2.4"
  }
}
