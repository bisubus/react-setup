{
  "name": "@react-setup/hooks",
  "version": "1.0.0",
  "description": "Easy initialization logic for React functional components, just like in Vue, Solid and Svelte",
  "keywords": ["react", "reactjs", "react-component", "react-components", "react-hooks", "setup", "typescript"],
  "author": "bisubus",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bisubus/react-setup.git"
  },
  "type": "module",
  "main": "dist/react-setup-hooks.umd.min.js",
  "module": "dist/react-setup-hooks.es.min.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/react-setup-hooks.es.min.js",
      "require": "./dist/react-setup-hooks.umd.min.js"
    }
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -b tsconfig.app.json && vite build --emptyOutDir && vite build -- --minify",
    "fix": "eslint --fix .",
    "lint": "eslint .",
    "test": "tsc --noEmit -p tsconfig.test.json && vitest --typecheck --run",
    "test:watch": "vitest --typecheck",
    "version": "npm version"
  },
  "devDependencies": {},
  "peerDependencies": {
    "@react-setup/core": "^1",
    "react": ">= 16.8"
  }
}
